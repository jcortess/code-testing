{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
      "no-console": 0,
      "prettier/prettier": 2,
      "no-unused-vars": 0,
      "no-await-in-loop":0,
      "no-confusing-arrow":2,
      "no-duplicate-imports":2,
      "no-dupe-args":2,
      "no-unreachable":2,
      "@typescript-eslint/no-unused-vars": "off"
    }
  }